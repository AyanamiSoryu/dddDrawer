{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "display": "Recommended",
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}